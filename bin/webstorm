#!/bin/bash
#Generated by JetBrains Toolbox 2.3.1.31116 at 2024-04-30T14:01:21.774948

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Applications/WebStorm.app/Contents/MacOS/webstorm" $wait --args "${intellij_args[@]}"